<!--以下是信息元各个参数的定义说明  -->
<数据结构定义 xmlns:BritNS="brit.software" 项目="YK-2311" 配置标识="{090908}" 版本="1.00">
	<!--以下是各个参数块的结构说明，参数类型有：char,uchar,short,ushort,bool,int,uint,long,ulong,byte,word,dword,float,double,
      hex,null,map,string,struct，refer等 
	  参数类型说明：
	  char(1字节有符号整数),
	  uchar(1字节无符号整数),
	  short(2字节有符号整数),
	  ushort(2字节无符号整数),
	  int(4字节有符号整数),
	  uint(4字节无符号整数),
	  byte(1字节无符号整数),
	  word(2字节无符号整数),
        dword(4字节无符号整数),
	  float(4字节浮点数),
	  double(8字节浮点数),
	  bool(4字节布尔数) ,
	  hex(表示一块十六进制数据，系数为一个块的字节长度，个数为总的块数，数据总字节长度=个数*系数)
	  map(表示一个映射关系，某个map元素的长度通过系数指明，对应的含义通过其中的映射表来确定，map只能是1.2.4)
	  null(表示一块无类型数据，不关心其内容，个数为该数据块的字节长度)
	  string(表示一个字符串，单一字符串的字节长度通过系数来指定,个数为字符串的个数)
	  struct(表示一个内嵌结构,个数表示结构数组的元素个数，每一个元素是一个结构，结构元素在其中定义)
	  refer(表示一个引用，实际上也是内嵌结构,与struct相似，个数表示结构数组的元素个数，每一个元素是一个结构，
	  与struct所不同的是引用的结构元素的定义为另一个参数块，ID值是所引用参数块的ID)-->
	<!--参数块中，ID是其唯一标识，十六进制表示，ID="E464.0000中,E464表示类型号，0000用于以后扩展的保留字段 -->
	<参数块 名称="校零信标一体机参数" ID="0xE614" 个数="1" 类型="常规缓冲区">
		<参数 名称="在线标志" ID="1" 个数="1" 类型="map" 系数="1" 单位="" 精度="" 最大值="" 最小值="">
			<值>1</值>
			<映射表>
				<映射项 数值="00H" 含义="校零信标都在线"/>
				<映射项 数值="01H" 含义="校零信标都不在线"/>
				<映射项 数值="02H" 含义="校零在线信标不在线"/>
				<映射项 数值="03H" 含义="信标在线"/>
			</映射表>
		</参数>
		<参数 名称="输入射频频率" ID="2" 个数="1" 类型="uint" 系数="1" 单位="MHz" 精度="" 最大值="2120" 最小值="2025">
			<值>2120</值>
		</参数>
		<参数 名称="输出射频频率" ID="3" 个数="1" 类型="uint" 系数="1" 单位="MHz" 精度="" 最大值="2400" 最小值="2200">
			<值>2200</值>
		</参数>
		<参数 名称="信标输出频率" ID="4" 个数="1" 类型="uint" 系数="1" 单位="MHz" 精度="" 最大值="2400" 最小值="2200">
			<值>2200</值>
		</参数>
		<参数 名称="数控衰减器1" ID="5" 个数="1" 类型="short" 系数="1" 单位="dB" 精度="" 最大值="60" 最小值="0">
			<值>0</值>
		</参数>
		<参数 名称="数控衰减器2" ID="6" 个数="1" 类型="short" 系数="1" 单位="dB" 精度="" 最大值="60" 最小值="0">
			<值>0</值>
		</参数>
		<参数 名称="参考源选择" ID="7" 个数="1" 类型="map" 系数="1" 单位="" 精度="" 最大值="" 最小值="">
			<值>1</值>
			<映射表>
				<映射项 数值="00H" 含义="内参考"/>
				<映射项 数值="01H" 含义="外参考"/>
			</映射表>
		</参数>
	</参数块>
</数据结构定义>
